    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.6",
    "vite": "4.4.9",
    "vue-tsc": "^1.8.3"
